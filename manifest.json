{
  "manifest_version": 3,
  "name": "Leetcode Helper",
  "description": "This extension is a useful helper when you code at leetcode.com and leetcode.cn",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*",
        "https://leetcode.cn/problems/*"
      ],
      "js": [
        "build/lhelper.js"
      ]
    }
  ],
  "permissions": []
}