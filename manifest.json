{
  "manifest_version": 2,

  "name": "Leetcode Helper",
  "description": "This extension is a useful helper when you code at leetcode.com.\n1.Copy the question for markdown.",
  "version": "0.1",

  // "browser_action": {
  //   "default_icon": "icon.png",
  //   "default_popup": "html/popup.html"
  // },
  "content_scripts": [
    {
      "matches": ["*://*.leetcode.com/*"],
      "css": [
        "lib/bootstrap/css/bootstrap.min.css",
        "lib/ionicons/css/ionicons.css",
        "css/main.css"
      ],
      "js": [
        "lib/jquery.js",
        "lib/bootstrap/js/bootstrap.min.js",
        "lib/clipboard.min.js",
        "lib/to-markdown.js",
        "js/helper/helper.js",
        "js/helper/helper-getter.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "lib/ionicons/fonts/*"
  ],
  "permissions": [
    "activeTab"
  ]
}
